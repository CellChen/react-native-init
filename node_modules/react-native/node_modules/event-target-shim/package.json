    "karma-browserify": "^4.2.1",
    "karma-chrome-launcher": "^0.1.12",
    "karma-firefox-launcher": "^0.1.6",
    "karma-growl-reporter": "^0.1.1",
    "karma-ie-launcher": "^0.1.5",
    "karma-mocha": "^0.1.10",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "npm-run-all": "^1.2.5",
    "power-assert": "^0.11.0",
    "rimraf": "^2.3.4",
    "spy": "^0.1.3",
    "uglify-js": "^2.4.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysticatea/event-target-shim.git"
